<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Signup</title>
    <style>
    
    .error-message {
        color: red; /* Set error messages to red color */
        margin-top: 5px
        }
        /* Reset default styles */
        body {
            margin: 0;
            padding: 0;
            font-family: Arial, sans-serif;
            background-color: #f5f5f5;
            background-image: url('data:image/jpeg;base64,')
        }

        /* Container for the signup form */
        .container {
            background-color: #fff;
            border-radius: 5px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            max-width: 400px;
            margin: 0 auto;
            padding: 20px;
            margin-top: 20px;
        }

        /* Form styling */
        h2 {
            text-align: center;
        }

        form {
            display: flex;
            flex-direction: column;
        }

        label {
            margin-bottom: 5px;
        }

        input[type="text"],
        input[type="email"],
        input[type="password"],
        input[type="tel"] {
            padding: 10px;
            margin-bottom: 10px;
            border: 1px solid #ccc;
            border-radius: 3px;
            font-size: 16px;
        }

        button[type="submit"] {
            background-color: #007bff;
            color: #fff;
            padding: 10px;
            border: none;
            border-radius: 3px;
            cursor: pointer;
            font-size: 18px;
            transition: background-color 0.3s;
        }

        button[type="submit"]:hover {
            background-color: #0056b3;
        }

        /* Media queries for responsiveness */
        @media (max-width: 600px) {
            .container {
                max-width: 100%;
            }
        }
    </style>
</head>

<body>
    <div class="container">
        <h2>Edit product</h2>
        <form action="/editproduct/<%= allproduct._id %>" method="POST" enctype="multipart/form-data" novalidate>


            <% /* Check if productname exists, if not, use an empty string */ %>
            <div>
                <label for="productname">Product Name</label>
                <input type="text" id="productname" name="productname" value="<%= allproduct.productname || '' %>"
                    required>
                    <% if (errorMessage) { %>
                        <p style="color: red;"><%= errorMessage %></p>
                    <% } %>
                </div>
                <label for="productprice">Product Price</label>
                <input type="number" id="productprice" name="productprice" value="<%= allproduct.productprice || '' %>"
                    required>

                <label for="productdescription">Product Description</label>
                <input type="text" id="productdescription" name="productdescription"
                    value="<%= allproduct.productdescription || '' %>" required>

                <label for="productstocks">Product Stocks</label>
                <input type="text" id="productstocks" name="productstocks" value="<%= allproduct.productstocks || '' %>"
                    required>

                <label for="productcategory">Product Category</label>
                <input type="text" id="productcategory" name="productcategory"
                    value="<%= allproduct.productcategory || '' %>" required>

                <label for="filename">Product Image</label>
                <input type="file" id="filename" name="filename" multiple>
                <div style="display: flex;width: 100%;flex-wrap: wrap;">
                    <% allproduct.productimage.forEach(img=> { %>
                        <img src="<%= img %>" alt="product image" style="width: 100px;height: 100px;">


                        <a href="/deleteimage?productId=<%= allproduct._id %>&imageUrl=<%= img %>">

                            Delete
                        </a>



                        <% }); %>

                </div>

                <button type="submit">SAVE</button>
        </form>
    </div>
    <script>


    </script>
    <script>
     document.querySelector('form').addEventListener('submit', function(event) {
   const productStock = document.getElementById('productstocks').value;
   const productPrice = document.getElementById('productprice').value;

   if (productStock <= 0) {
       event.preventDefault();
       document.getElementById('productstocks').setCustomValidity('Product stock must be greater than 0');
   } else {
       document.getElementById('productstocks').setCustomValidity('');
   }

   if (productPrice < 1) {
       event.preventDefault();
       document.getElementById('productprice').setCustomValidity('Product price must be greater than or equal to 1');
   } else {
       document.getElementById('productprice').setCustomValidity('');
   }
});

     </script>
     <script>
        document.addEventListener('DOMContentLoaded', function () {
            const editProductForm = document.getElementById('editProductForm');
            const productNameInput = document.getElementById('productname');
            const originalProductNameInput = document.getElementById('originalProductName');

            editProductForm.addEventListener('submit', function (event) {
                if (productNameInput.value.trim().toLowerCase() === originalProductNameInput.value.trim().toLowerCase()) {
                    event.preventDefault();
                    alert('Product name cannot be the same as the original product name. Please choose a different name.');
                    
                }
            });
        });
    </script>


</body>

</html>