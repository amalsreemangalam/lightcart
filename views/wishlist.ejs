	<!-- cart + summary -->
	<!-- cart + summary -->
	<section class="bg-light my-5">
		<div class="container">
			<div class="row">
				<!-- cart -->
				<div class="col-lg-9">
					<div class="card border shadow-0">
						<div class="m-4">
								<% if ( product&&product.length===0) { %>
									<h1>Wishlist is Empty</h1>


									<% } else if(product) { %>
										<h4 class="card-title mb-4">Your Wishlist</h4>
										<% console.log('wishproos',product); %>

										<% product.forEach(item=> { %>
											<div class="row gy-3 mb-4">
												<div class="col-lg-5">
													<div class="me-lg-5">
														<div class="d-flex">


															<% console.log('item:', item.Product.productname); %>
																<img src="<%= item.Product && item.Product.productimage && item.Product.productimage.length > 0 ? item.Product.productimage[0] : 'default-image.jpg' %>"
																	class="border rounded me-3"
																	style="width: 96px; height: 96px;" />


																<div class="">
																	<a href="#" class="nav-link">
																		<h3>
																			<%= item.Product.productname %>
																		</h3>
																	</a>
																	<p class="text-muted">Category: <%=
																			item.Product.productname %>
																	</p>
																	<!-- <p class="text-muted" >Quantity:</p> -->
																</div>
														</div>
													</div>
												</div>





												<div
													class="col-lg-2 col-sm-6 col-6 d-flex flex-row flex-lg-column flex-xl-row text-nowrap">





													<div class="">
														<%if(item.Product.OfferPrice){%>
															<text class="h5">Price:₹<s>
																	<%= item.Product.productprice%>
																</s> &#8377;<%= item.Product.OfferPrice %></text> <br />
															<% }else{ %>%>
																<text class="h5">Price:₹<%= item.Product.productprice%>
																		</text>
																<% } %>
													</div>
												</div>


												<div
													class="col-lg col-sm-6 d-flex justify-content-sm-center justify-content-md-start justify-content-lg-center justify-content-xl-end mb-2">
													<div class="float-md-end me-2">
														<!-- Added 'me-2' class for right margin -->
														<a href="/cart/<%=item.Product._id%>"
															class="btn btn-light border text-success icon-hover-success">Add
															To Cart</a>
													</div>
													<div class="float-md-end">
														<a href="/wishlist/remove/<%=item.Product._id%>"
															class="btn btn-light border text-danger icon-hover-danger">Remove</a>
													</div>
												</div>

											</div>

											<br />

											<% }); %>
						</div>
						<!-- Other cart details can be added here -->
					</div>
				</div>


			</div>


			<% } %>




				<br><br><br><br><br><br><br><br>
	</section>
